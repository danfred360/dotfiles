#==git
alias g='git'
alias gitcommit='git c'
alias gitcommitall='git call'

#--count
alias gitcount='git count'

#--logging
#---short
alias gitl='git l'
#---default
alias gitlog='git lg'
#---with graph
alias gitlogg='git lg'
#---with names
alias gitlogn='git logn'
#---with diff
alias gitlogp='git lop'

#==svn
alias svnstatus="svn status --ignore-externals"
alias svnstatusa="svn status"
alias svnupdate="svn update --ignore-externals"
alias svnupdatea="svnupdate"
